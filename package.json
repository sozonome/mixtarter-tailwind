{
  "private": true,
  "name": "mixtarter-tailwind",
  "description": "",
  "author": "sozonome",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup vercel",
    "lint": "eslint app",
    "lint:fix": "eslint app --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write app",
    "up-latest": "yarn upgrade-interactive --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "start": "remix-serve build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/serve": "^1.4.0",
    "@remix-run/vercel": "^1.4.0",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.4",
    "@remix-run/dev": "^1.4.0",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.3",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.11.0",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.8",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
